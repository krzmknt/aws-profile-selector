{
  "name": "aws-profile-selector",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Interactive AWS profile picker",
  "type": "module",
  "bin": {
    "aws-profile-selector": "./dist/bin/selector.js",
    "aws-profile-selector-init": "./dist/bin/init.js"
