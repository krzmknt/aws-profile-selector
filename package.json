    "@typescript-eslint/parser": "^8.34.1",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.0.2",
    "prettier": "^3.2.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10",
    "typescript": "^5"
